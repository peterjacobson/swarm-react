{
  "name": "swarm",
  "version": "1.0.0",
  "description": "unblocker",
  "main": "index.js",
  "scripts": {
    "start": "budo index.js -o bundle.js --live",
    "build": "browserify index.js -o bundle.js -g uglifyify",
    "test": "run-browser test/* -b",
    "deploy": "gh-pages-deploy"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ]
    ]
  },
  "gh-pages-deploy": {
    "staticpath": ".",
    "prep": "build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peterjacobson/swarm"
  },
  "author": "peterjacobson",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/peterjacobson/swarm/issues"
  },
  "homepage": "https://github.com/peterjacobson/swarm",
  "dependencies": {
    "jquery": "^2.1.4",
    "react": "^0.13.3",
    "reactify": "^1.1.1"
  },
  "devDependencies": {
    "browserify": "^9.0.7",
    "budo": "^2.1.3",
    "gh-pages-deploy": "^0.3.0",
    "run-browser": "^2.0.2",
    "tape": "^4.0.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.1.0"
  }
}
