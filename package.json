  "bugs": {
    "url": "https://github.com/peterjacobson/swarm/issues"
  },
  "homepage": "https://github.com/peterjacobson/swarm",
  "dependencies": {
    "alt": "^0.16.10",
    "jquery": "^2.1.4",
    "react": "^0.13.3",
    "react-flexbox": "^2.0.0",
    "reactify": "^1.1.1"
  },
  "devDependencies": {
    "babelify": "^6.1.2",
    "browserify": "^9.0.7",
    "budo": "^2.1.3",
    "gh-pages": "^0.3.0",
    "run-browser": "^2.0.2",
    "tape": "^4.0.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.1.0"
