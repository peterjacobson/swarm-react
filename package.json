{
  "name": "swarm",
  "version": "1.0.0",
  "description": "unblocker",
  "main": "index.js",
  "scripts": {
    "start": "budo index.js -o bundle.js --live",
    "build": "browserify index.js -o bundle.js -g uglifyify",
